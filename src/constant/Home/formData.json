            { "value": "in-nashik", "label": "India (Nashik)" },
            { "value": "in-noida", "label": "India (Noida)" },
            { "value": "uae-dubai", "label": "UAE (Dubai)" },
            { "value": "sg-singapore", "label": "Singapore (Singapore)" },
            { "value": "au-milburn", "label": "Australia (Milburn)" }
