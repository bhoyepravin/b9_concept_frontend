      },
      {
        "feature": "Guided Integration",
        "packages": [
          "Self-Guided Plan",
          "1 Follow-up Session",
          "Weekly (12 Weeks)"
        ],
        "rowClass": "bg-white"
      },
      {
        "feature": "Practitioner Access",
        "packages": [
          "Standard Email",
          "30-Day Priority Access",
          "90-Day Concierge"
        ],
        "rowClass": "bg-gray-50"
      },
      {
        "feature": "Recovery Plan",
        "packages": ["Cognitive", "Custom", "Executive Performance Plan"],
        "rowClass": "bg-white"
      }
    ]
  }
}
