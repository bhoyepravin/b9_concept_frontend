{
  "section": {
    "containerClasses": "max-w-6xl mx-auto bg-light-gray p-8 rounded-lg shadow-lg overflow-x-auto",
    "title": "Package Comparison",
    "titleClasses": "text-2xl font-semibold font-montserrat text-deep-blue text-center mb-6"
  },
  "table": {
    "classes": "w-full text-left border-collapse min-w-[700px]",
    "header": {
      "rowClasses": "border-b-2 border-gray-300",
